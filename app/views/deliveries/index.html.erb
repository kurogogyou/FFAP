<h1>Listing deliveries</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Delivery User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @deliveries.each do |delivery| %>
      <tr>
        <td><%= delivery.order.id %></td>
        <td><%= delivery.user.username %></td>
        <td><%= link_to 'Show', delivery %></td>
        <td><%= link_to 'Edit', edit_delivery_path(delivery) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

