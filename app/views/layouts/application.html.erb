      <div id="welcome">
        <ul>
          <% if session %>
            <h2>Welcome, <%= current_user.username %></h2>
          <% end %>
        </ul>
      </div>

      <div class="cssmenu">
        <ul>
          <!--  | -->

          <% if session %>
            <li><%= link_to 'Edit user', edit_user_path(:current) %></li> |
            <li><%= link_to 'Logout', logout_path, method: :delete%></li>
          <% else %>
            <li><%= link_to 'Register', register_path %></li> |
